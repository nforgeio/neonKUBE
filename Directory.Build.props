<Project>
	
	<!--
	Default properties:
	-->

	<PropertyGroup>
		<Product>neonSDK</Product>
		<Authors>neonFORGE Team</Authors>
		<Company>neonFORGE LLC</Company>
		<Copyright>Copyright Â© 2005-2022 by neonFORGE LLC.  All rights reserved.</Copyright>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<LangVersion>latest</LangVersion>
		<Nullable>disable</Nullable>
		<AnalysisLevel>5.0</AnalysisLevel>
		<DebugType>portable</DebugType>
		<TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	</PropertyGroup>

	<!--
	Disable generation of attribute files in the project [obj/CONFIG] folders
	because this can cause C# duplicate symbol errors when switching between
	DEBUG and RELEASE builds.
	-->

	<PropertyGroup>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
	</PropertyGroup>

	<!-- 
	Remove generated [**/obj/**/*.cs] files to avoid duplicate symbol compilation 
	errors when these generated files exist in multiple build configurations.
	-->

	<Target Name="CleanGeneratedCs" BeforeTargets="Clean" Condition="'$(ProjectDir)' != ''">
		<Message Importance="high" Text="CLEAN: neon-build clean-generated-cs $(ProjectDir)" />
		<Exec Command="neon-build clean-generated-cs &quot;$(ProjectDir)&quot;" />
	</Target>

</Project>
